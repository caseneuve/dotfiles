
# manjaro local settings:
# ======================
# Last update: 07.06.18, 21:11:37 @manjaroi3

bindsym $mod+Shift+Delete       exec shutdown now
bindsym $mod+Shift+Home         exec systemctl hibernate
bindsym $mod+Shift+End          exec --no-startup-id ~/.dotfiles/i3/i3lock/lock.sh; exec systemctl suspend
bindsym $mod+Pause              exec systemctl suspend

bindsym shift+F12                exec lk4b

# zmienne dla hangout√≥w
set $right 990
set $down 505
# sys updt window
set $sright 750
# video mode
set $vright 950
set $vtop 30
# yt-download
set $yright 850

